class Solution {
    public int maxSubArray(int[] nums) {
      int maxsum=nums[0];
      int c=0;
        for(int n:nums)
        {
            if(c<0)
            {
                c=0;
            }
            c+=n;
            maxsum=Math.max(maxsum,c);
        }
        return maxsum;
    }
}
